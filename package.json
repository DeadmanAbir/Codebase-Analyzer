{
  "name": "ai-codebase-analyzer",
  "displayName": "AI Codebase Analyzer",
  "description": "AI-powered codebase analysis and review tool with interactive chat - no setup required!",
  "version": "1.1.3",
  "publisher": "abirdutta",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Programming Languages"
  ],
  "keywords": [
    "ai",
    "codebase",
    "analysis",
    "code review",
    "assistant",
    "no setup"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "./assests/icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeadmanAbir/Codebase-Analyzer"
  },
  "bugs": {
    "url": "https://github.com/DeadmanAbir/Codebase-Analyzer/issues"
  },
  "homepage": "https://github.com/DeadmanAbir/Codebase-Analyzer#readme",
  "license": "MIT",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codebase-analyzer",
          "title": "AI Codebase Analyzer",
          "icon": "$(robot)"
        }
      ]
    },
    "views": {
      "codebase-analyzer": [
        {
          "type": "webview",
          "id": "codebase-analyzer.chatView",
          "name": "AI Assistant",
          "when": "true",
          "icon": "$(robot)"
        }
      ]
    },
    "commands": [
      {
        "command": "codebase-analyzer.analyzeCodebase",
        "title": "Analyze Codebase with AI",
        "category": "AI Codebase Analyzer"
      },
      {
        "command": "codebase-analyzer.clearChat",
        "title": "Clear Chat",
        "icon": "$(clear-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "codebase-analyzer.clearChat",
          "when": "view == codebase-analyzer.chatView",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "codebase-analyzer.analyzeCodebase",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "compile:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@langchain/community": "^0.3.53",
    "@langchain/core": "^0.3.72",
    "@langchain/langgraph": "^0.4.6",
    "@langchain/openai": "^0.6.9",
    "axios": "^1.11.0",
    "zod": "^3.25.76"
  }
}
